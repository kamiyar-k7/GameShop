@model Application.ViewModel.AdminSide.AdminOrdersViewModel;



    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="product-status-wrap">
            <h4>Products List</h4>
           
            <table>
                <tbody>


                    <tr>

                        <th>Image</th>
                        <th>Product Title</th>
                        <th>Price</th>
                        <th>Quantity</th>
                        <th>Platfrom</th>
                       
                    </tr>
                   
                     @foreach (var item in Model.cartDeatails)
                    {
                        <tr>

                            <td><img src="~/assets/images/GameImages/@item.ItemImage" alt=""></td>

                            <a>
                            <td>@item.GameName</td> </a>
                            <td>@item.Price</td>
                            <td>@item.Quantity</td>
                            <td>@item.Platform</td>

                            
                        </tr>
                    }
                  

                    
                   
                </tbody>
            </table>

            <div class="custom-pagination">
                <ul class="pagination">
                    <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                    <li class="page-item"><a class="page-link" href="#">Next</a></li>
                </ul>
            </div>

        <div class="footer-copyright-area">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="text-center custom-pro-edt-ds">

<form method="post" asp-area="Admin" asp-controller="Orders" asp-action="ChangeStatus">
    <select asp-for="OneOrder.Status" style="margin-bottom:10px;" class="form-control pro-edt-select form-control-primary">
        <option value="">Select Status</option>

        @foreach (var status in Html.GetEnumSelectList<Domain.entities.Cart.OrderStatus>())
        {
            <option value="@status.Value">@status.Text</option>
        }
    </select>
    <input hidden asp-for="OneOrder.CartId" />

    <button type="submit" class="btn btn-primary">Save</button>
</form>

                  

                        </div>
                    </div>

                </div>
            </div>
        </div>
        </div>
    </div>
